
.headers{
	height: 115rpx;
}
.nav_top{
	width: 100vw;
	height: 115rpx;
	background-color: white;
	position: fixed;
	top: 0;
	left: 0;
	display: flex;
	flex-direction: column;
	align-items: center;
	z-index: 800;
}
.status_bar{
	width: 100vw;
	height: 64px;
}
.topContent{
	width: 100vw;
	height: 100px;
	display: flex;
	align-items: center;
}
.goBack{
	width: 90rpx;
	height: 42rpx;
	padding-left: 20rpx;
	box-sizing: border-box;
	position: relative;
	font-size: 24rpx;
}
.goBack .imgbg{
	position: absolute;
	left: 50%;
	top: 50%;
	-webkit-transform: translate(-50%, -50%);
	        transform: translate(-50%, -50%);
	width: 40rpx;
	height: 42rpx;
}
.title{
	line-height: 115rpx;
	font-size: 32rpx;
	text-align: center;
	color: #344356;
	position: absolute;
	left: 50%;
	/* top: 50%; */
	-webkit-transform: translate(-50%, 0);
	        transform: translate(-50%, 0);
}

